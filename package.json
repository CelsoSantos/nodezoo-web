{
  "name": "nodezoo-web",
  "version": "4.0.4",
  "description": "Frontend for NodeZoo",
  "author": "Richard Rodger",
  "contributors": [
    "Richard Rodger",
    "Dean McDonnell",
    "Karolina Szczur"
  ],
  "main": "./server.start.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rjrodger/nodezoo-web.git"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "lab test/*.test.js -r console -v -L -m 3000",
    "lint": "lab test -dL",
    "copy": "copyfiles -f ./client/assets/img/favicon.png ./dist/img",
    "build": "npm run copy & webpack --progress --colors",
    "watch": "webpack -w",
    "start": "node -r toolbag server/start.js --seneca.options.tag=nodezoo-web --seneca.options.debug.short_logs=true --seneca.log=type:act",
    "prepublish": "in-publish && npm run build || not-in-publish"
  },
  "dependencies": {
    "boom": "3.x.x",
    "chairo": "2.1.0",
    "hapi": "12.0.0",
    "history": "1.17.x",
    "in-publish": "^2.0.0",
    "inert": "3.x.x",
    "lodash": "3.10.1",
    "moment": "2.11.x",
    "react": "0.14.x",
    "react-copy-to-clipboard": "^4.0.2",
    "react-dom": "0.14.x",
    "react-redux": "4.0.x",
    "react-router": "1.0.x",
    "redux": "3.0.x",
    "redux-logger": "2.3.x",
    "redux-simple-router": "1.0.x",
    "redux-thunk": "1.0.x",
    "seneca": "2.0.0",
    "seneca-balance-client": "0.4.0",
    "seneca-entity": "0.0.1",
    "seneca-mesh": "0.7.0",
    "style-loader": "0.13.x",
    "superagent": "1.6.x",
    "toolbag": "2.0.0",
    "toolbag-plugin-stats-collector": "1.0.0",
    "toolbag-plugin-udp-reporter": "1.0.0",
    "vidi-metrics": "0.1.0",
    "vidi-seneca-metrics": "0.2.0"
  },
  "devDependencies": {
    "babel-core": "6.4.x",
    "babel-eslint": "^4.1.8",
    "babel-loader": "6.2.x",
    "babel-plugin-react-transform": "2.0.x",
    "babel-plugin-transform-decorators-legacy": "1.x.x",
    "babel-plugin-transform-object-rest-spread": "6.3.x",
    "babel-preset-es2015": "6.3.x",
    "babel-preset-react": "6.3.x",
    "babel-preset-stage-0": "6.3.x",
    "copyfiles": "^0.2.1",
    "css-loader": "0.23.x",
    "eslint-config-seneca": "1.1.2",
    "eslint-plugin-standard": "1.3.1",
    "eslint-plugin-react": "3.16.x",
    "file-loader": "0.8.x",
    "lab": "8.0.2",
    "postcss": "5.0.14",
    "postcss-loader": "0.8.0",
    "stylus-loader": "1.4.3",
    "url-loader": "0.5.7",
    "webpack": "1.12.x"
  }
}
